
digraph D {
  graph[fontname="helvetica" fontsize=9 fontcolor="#000060"]
  rankdir=LR
  bgcolor=yellow
  edge[color=gray fontname="times-italic" fontsize=10]
  node[fontname="helvetica" fontsize=10]

  /* 3: process P1 */
  "P1" [shape=ellipse label="P1" fillcolor="#eeeeee" style=filled ]

  /* 4: process P2 */
  "P2" [shape=ellipse label="P2" fillcolor="#eeeeee" style=filled ]

  /* 5: flow P1 P2 */
  "P1" -> "P2" [label=""]
}